{
  "scripts": {
    "clean-site-build": "rimraf ./build/site/build",
    "_build-site-build": "tsc -p ./src/site/build/bin/tsconfig-dist.json",
    "build-site-build": "npm run clean-site-build && npm run _build-site-build",

    "watch-site-client-dev": "env-cmd -e dev node ./build/site/build/site/build/bin/watchClient.js",
    "watch-site-server-dev": "env-cmd -e dev node ./build/site/build/site/build/bin/watchServer.js",

    "clean-comp-site-build": "rimraf ./build/comp-site/build",
    "_build-comp-site-react-build": "tsc -p ./src/comp-site/react/build/bin/tsconfig-dist.json",
    "build-comp-site-react-build": "npm run clean-comp-site-build && npm run _build-comp-site-react-build",
    "watch-comp-site-react-dev": "env-cmd -e dev node ./build/comp-site-react-watch/comp-site/react/build/bin/watch.js",

    "build-intercom-server": "tsc -p ./src/intercom/bin/tsconfig-dist.json",
    "intercom-server": "env-cmd -e dev node ./build/intercom-server/intercom/bin/server.js",

    "build-start": "concurrently -m 3 -n \"bsb ,bcsb,bis \" -c \"red,green,blue\" \"npm run build-site-build\" \"npm run build-comp-site-react-build\" \"npm run build-intercom-server\"",
    "_start": "concurrently -m 4 -n \"comp-site,site-ui  ,site-api ,intercom \" -c \"red,green,blue,cyan\" \"npm run watch-comp-site-react-dev\" \"npm run watch-site-client-dev\" \"npm run watch-site-server-dev\" \"npm run intercom-server\"",
    "start": "npm run build-start && npm run _start",

    "cli-dev": "env-cmd -e dev node ./build/cli/cli/index.js start --verbose",
    "cli-dev-rel-config": "env-cmd -e dev-rel node ./build/cli/cli/index.js start -c ./test/exh.config.json",
    "cli-dev-config": "env-cmd -e dev node ./build/cli/cli/index.js start -c ./test/exh.config.json --verbose",
    "cli-dev-config-js": "env-cmd -e dev node ./build/cli/cli/index.js start -c ./test/exh.config.js --verbose",
    "cli-dev-config-ts": "env-cmd -e dev node ./build/cli/cli/index.js start -c ./test/exh.config.ts --verbose"
  },
