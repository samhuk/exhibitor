{
  "$schema": "http://json.schemastore.org/tsconfig",
  "include": [
    "./index.d.ts",
    "./common.tsx"
  ],
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "module": "ES6",
    "allowJs": false,
    "strictNullChecks": false,
		"strict": true,
		"isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "sourceMap": false,
    "noEmit": false,
    "skipLibCheck": true,
    "jsx": "react"
  }
}
